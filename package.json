{
  "name": "playwright-project",
  "version": "1.0.0",
  "type": "module",
  "description": "Playwright TypeScript project with tests and fixtures",
  "main": "index.js",
  "scripts": {
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
